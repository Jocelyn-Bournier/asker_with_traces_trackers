<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity
  repository-class="SimpleIT\ClaireExerciseBundle\Repository\PedagogicRepository"
  name="SimpleIT\ClaireExerciseBundle\Entity\Pedagogic">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="code" type="string" column="code" length="20"/>
    <field name="year" type="integer" column="year"/>
    <field name="longName" type="string" column="longName" length="100"/>
    <field name="period" type="string" column="period" length="50" nullable="true"/>
    <many-to-many field="users" mapped-by="pedagogics"
    target-entity="SimpleIT\ClaireExerciseBundle\Entity\AskerUser">
    </many-to-many>
    <unique-constraints>
      <unique-constraint columns="code,year,period" name="search_idx_code_year_period"/>
    </unique-constraints>
  </entity>
</doctrine-mapping>
