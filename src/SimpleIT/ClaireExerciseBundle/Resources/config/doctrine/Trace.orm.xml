<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SimpleIT\ClaireExerciseBundle\Entity\Trace" table="interaction_traces">
    <id name="interactionId" type="integer" column="interaction_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dd" type="datetime" column="dd" nullable="false"/>
    <field name="df" type="datetime" column="df" nullable="false"/>
    <field name="content" type="text" column="content" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="context" type="text" column="context" length="0" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
